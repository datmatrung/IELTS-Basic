Unit 1:
  /i/ = i, ui, y
  ship, kid, picture, happy, big, build, lipstick, 
  drink, relax, with, family, crazy, pretty, city,
  hit, become, win, guitar, miss, decide, kiss, pick, 
  cinema, listen, dinner, ticket, swim, busy, dictionary,
  system, think, chicken, lift, sixty, minute, english,
  window, biscuit, pink, message
  
  /i:/ = ee, ie, ei, e, ese, ea
  employee, easy, see, he, she, leave, steal, tea, 
  team, feel, cheap, coffee, machine, cheese, teach,
  keep, thief, pizza, meat, agree, free, team, peach, 
  cream, chinese, speak, police, magazine, screen
  
Unit 2:
  /u/ = u
  woman, book, foot, full, cook, room, flute, should,
  put, good, push, cushion, look, would, sugar
  
  /u:/ = oo
  bamboo, afternoon, pool, food, tooth, group, blue, too, two,
  fruit, juice, choose, tuesday, school, soup, spoon, lunar, june  
  
Unit 3:
  /o/ = o, a
  /o:/ = a, al, au, aw, augh, ough, ar, o, oor, ore, our
